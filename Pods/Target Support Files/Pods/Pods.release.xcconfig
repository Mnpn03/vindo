OTHER_LDFLAGS = $(inherited) -ObjC -l"CDEvents" -l"FXNotifications" -l"ReactiveCocoa" -l"Rebel" -framework "Cocoa" -framework "CoreServices" -framework "QuartzCore" -framework "Sparkle" -framework "WebKit"
