---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:sparkle="http://www.andymatuschak.org/xml-namespaces/sparkle"  xmlns:dc="http://purl.org/dc/elements/1.1/">
    <channel>
        <title>Vindo Changelog</title>
        <link>https://vindo-app.github.io/appcast.xml</link>
        <description>Vindo Appcast</description>
        <language>en</language>
        {% assign release = site.github.releases | first %}
        <item>
            <title>Vindo {{ release.name }}</title>
            <description>
                <![CDATA[{{ release.body | markdownify }}]]>
            </description>
            {% assign asset = release.assets | first %}
            <enclosure url="{{ asset.browser_download_url }}" sparkle:version="{{ release.tag_name }}" length="{{ asset.size }}" type="application/octet-stream" />
        </item>
    </channel>
</rss>
