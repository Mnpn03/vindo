---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:sparkle="http://www.andymatuschak.org/xml-namespaces/sparkle"  xmlns:dc="http://purl.org/dc/elements/1.1/">
    <channel>
        <title>Vindo Changelog</title>
        <link>https://vindo-app.github.io/appcast.xml</link>
        <description>Vindo Appcast</description>
        <language>en</language>
        {% for version in site.versions %}
            <item>
                <title>Vindo {{ version.version }}</title>
                <description>
                    <![CDATA[
                        {{ version.content }}
                    ]]>
                </description>
                <enclosure url="{{ version.file_url }}" sparkle:version="{{ version.version }}" length="{{ version.file_size }}" type="application/octet-stream" />
            </item>
        {% endfor %}
    </channel>
</rss>
